    <haLatitudine rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">37.458957</haLatitudine>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.semanticweb.org/alessiodesktop/ontologies/2019/0/untitled-ontology-10#R85">
