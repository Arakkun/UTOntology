  <rdf:Description rdf:about="http://www.semanticweb.org/alessiodesktop/ontologies/2019/0/untitled-ontology-10#R49">
    <haDimensioneA rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">30</haDimensioneA>
